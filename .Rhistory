group_by(utterance_id, cv_label) %>%
summarise(pairs=n()-1)
npvi_v_sum1 <- df %>% filter(cv_label==v_label) %>%
select(utterance_id, cv_label, cv_duration) %>%
group_by(utterance_id, cv_label) %>%
mutate(npvi_sum = cv_duration + lag(cv_duration),
seq = row_number() - 1)
npvi_v_sum2 <- npvi_v_sum1 %>% group_by(utterance_id, cv_label) %>% summarise(sum = sum(npvi_sum, na.rm = T)/2)
npvi_v_diff1 <- df %>% filter(cv_label==v_label) %>%
group_by(utterance_id, cv_label) %>%
mutate(diff = cv_duration - lag(cv_duration))
npvi_v_diff2 <- npvi_v_diff1 %>% group_by(utterance_id, cv_label) %>% summarise(diff = sum(diff, na.rm = T))
npvi_v1 <- left_join(npvi_v_diff2, npvi_v_pair, by=c('utterance_id', 'cv_label'))
npvi_v2 <- left_join(npvi_v1, npvi_v_sum2, by=c('utterance_id', 'cv_label'))
npvi_v3 <- npvi_v2 %>% mutate(npvi_v=(abs(diff)/sum/pairs)*100)
plot <- ggplot(npvi_v3, aes(x=cv_label, y=npvi_v, fill=cv_label)) +
geom_boxplot(show.legend = F)+ scale_fill_jco()+ labs(y= 'nPVI-V', x = 'Vowel')
